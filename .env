

# API Configuration
API_BASE_URL=https://flashback.inc:9000/api/mobile

# Authentication
REFRESH_TOKEN=my-token-example

# Development Settings
USE_MOCK_API=false

# App Configuration
APP_NAME=Flashback
APP_VERSION=1.0.0

# Feature Flags
ENABLE_LIVENESS_CHECK=true
ENABLE_SELFIE_UPLOAD=true
ENABLE_DEV_SKIP_OPTIONS=false

# Timeouts
OTP_TIMEOUT=30000
LIVENESS_CHECK_DURATION=8000
UPLOAD_TIMEOUT=60000

# Validation
PHONE_REGEX=^\+91[6-9]\d{9}$
OTP_LENGTH=6

# UI Configuration
ANIMATION_DURATION=300
DEBOUNCE_DELAY=500

# File Upload Configuration
MAX_FILE_SIZE=10485760
COMPRESSION_QUALITY=0.8

# Liveness Detection Configuration
MIN_BLINK_COUNT=3
MIN_FACE_DETECTION_RATE=0.8
MAX_CONSECUTIVE_NO_FACE_FRAMES=3
FRAME_PROCESSING_INTERVAL=200
CHECK_DURATION=8000
